<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:element name="title" type="xs:string"/>
	<xs:element name="description" type="xs:string"/>
	<xs:element name="icon" type="xs:string"/>
	<xs:element name="buttonText" type="xs:string"/>
	
    <xs:element name="forest">
    	<xs:complexType>
    		<xs:sequence>
    			<xs:element ref="title"/>
    			<xs:element ref="trunk" minOccurs="1" maxOccurs="unbounded"/>
    		</xs:sequence>
    	</xs:complexType>
    </xs:element>

	<xs:element name = "trunk">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="title"/>
				<xs:element ref="description"/>
				<xs:element ref="icon"/>
				<xs:element ref="buttonText"/>
				<xs:element name="trunkTag" type="xs:string"/>
				<xs:element name="area" type="xs:string"/>
				<xs:element name="urgency" type="xs:positiveInteger" default="1"/>
				<xs:element name="deck" type="xs:string"/>
				<xs:element ref="branch" minOccurs="1" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="branch">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="title"/>
				<xs:element ref="description"/>
				<xs:element ref="icon"/>
				<xs:element ref="buttonText"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="branches">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="branch" minOccurs="1" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	

</xs:schema>